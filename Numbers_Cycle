using System;

namespace First_blood
{
    class Program
    {
        static void Main(string[] args)
        {
            int startNumber = 5;
            int increasedNumber = 7;
            int maxNumber = 100;

            for (int i = startNumber; i < maxNumber; i += increasedNumber)
            {
                Console.Write(i + " ");
            }
        }
    }
}
